{
  "name": "frost",
  "version": "0.1.13",
  "description": "Open source Vercel alternative",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "migrate": "bun scripts/migrate.ts",
    "setup": "bun src/scripts/setup.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "db:gen": "node ./node_modules/.bin/kysely-gen --url ./data/frost.db --dialect sqlite --out ./src/lib/db-types.ts",
    "test": "bun test"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "kysely": "^0.27.4",
    "kysely-bun-worker": "^1.2.1",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.0.9",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.3.5",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "kysely-gen": "^0.9.1",
    "postcss": "^8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2"
  }
}
